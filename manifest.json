{
  "name": "Gmail Dynamic From Sender",
  "description": "Set the From address dynamically according to set rules",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": ["inboxsdk.js","content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage",
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "options_page": "options.html",
  "manifest_version": 2
}
